{
  "name": "site-musica",
  "version": "1.0.0",
  "main": "index.js",
  "description": "",
 "scripts": {
  "test": "echo \"Error: no test specified\" && exit 1",
  "minify:js": "terser src/app.js -o dist/app.min.js --compress drop_console=true --mangle",
   "obfuscate:js": "javascript-obfuscator dist/app.min.js --output dist/app.obf.js --compact true --control-flow-flattening true",
  "minify:css": "cleancss -o dist/style.min.css src/style.css",
  "minify:html": "html-minifier --collapse-whitespace --remove-comments --minify-css true --minify-js true -o dist/index.html src/index.html",
  "copy:images": "cp -r src/images dist/",
  "build": "npm run minify:js && npm run minify:css && npm run minify:html && npm run copy:images && npm run obfuscate:js"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.7.0",
    "papaparse": "^5.4.1",
    "terser": "^5.20.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "html-minifier": "^4.0.0",
    "javascript-obfuscator": "^5.0.0"
  }
}
